    "description": "A basic cache implementation with adapter support through PSR-6 pools",
    "license": "MIT",
    "homepage": "http://docs.talesoft.codes/php/tale/inflector",
    "authors": [
        {
            "name": "Torben Koehn",
            "email": "torben@talesoft.codes"
        }
    ],
    "require": {
        "php": ">=7.1.0",
